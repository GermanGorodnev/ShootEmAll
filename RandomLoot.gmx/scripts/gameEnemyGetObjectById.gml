///gameEnemyGetObjectById(ENEMY.__ id)
switch (argument[0])
{
case ENEMY.__PILLOW:
    return oPillow;
    break;
case ENEMY.__JELLY:
    return oJelly;
    break;
case ENEMY.__OCTO_HEALER:
    return oOctoHealer;
    break;
case ENEMY.__OCTO_GUARD_MACE:
    return oOctoClub;
    break;
case ENEMY.__OCTO_ATTACK:
    return oOctoAttack;
    break;
case ENEMY.__OCTO_GUARD:
    return oOctoGuard;
    break;
}
